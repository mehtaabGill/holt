      "default": "./dist/src/index.js"
    }
  },
  "types": "./dist/src/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "bun --watch ./src/index.ts",
    "test": "bun test",
    "build": "rm -rf ./dist && bun run ./build.ts"
  },
  "devDependencies": {
    "bun-types": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "elysia": ">=0.6.24"
