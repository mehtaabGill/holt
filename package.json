{
  "name": "@tlscipher/holt",
  "version": "0.0.5",
  "description": "A highly configurable logger middleware for ElysiaJS.",
  "type": "module",
  "author": {
    "url": "https://github.com/mehtaabGill",
    "email": "hello@mehtaab.dev",
    "name": "Mehtaab Gill"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mehtaabGill/holt"
  },
  "private": false,
  "keywords": [
    "elysia",
    "bun",
    "logger",
    "middleware",
    "elysiaJS",
    "holt",
    "tlscipher"
  ],
  "homepage": "https://github.com/mehtaabGill/holt",
  "bugs": "https://github.com/mehtaabGill/holt/issues",
  "main": "./dist/src/index.js",
  "module": "./dist/src/index.js",
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
